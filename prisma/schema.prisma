generator client {
  provider = "prisma-client-js"
  output  = "../src/core/data/prisma/ledminh-dev-client"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("LEDMINH_DEV_DATABASE_URL")
}


model Message {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  name      String
  email     String
  message   String
}