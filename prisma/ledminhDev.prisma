generator client {
  output = "../node_modules/@prismany/client/base_schema"
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("LEDMINH_DEV_DATABASE_URL")
}


model Message {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  name      String
  email     String
  message   String
}